{
  "name": "sirespo",
  "version": "0.0.1",
  "description": "React app that uses webpack-dev-server for development, node for production",
  "main": "server/main.js",
  "scripts": {
    "compile": "NODE_ENV=production webpack",
    "format": "prettier src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prod": "NODE_ENV=production webpack && NODE_ENV=production node server/main.js",
    "start": "NODE_ENV=development node server/main.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "clearMocks": true,
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/jest.index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost"
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "compression": "^1.6.2",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.15.2",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "regenerator-runtime": "^0.12.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^4.10.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "mini-css-extract-plugin": "^0.4.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  }
}
