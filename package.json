{
  "name": "sirespo",
  "version": "0.0.1",
  "description": "React app that uses webpack-dev-server for development, node for production",
  "main": "server/main.js",
  "scripts": {
    "compile": "NODE_ENV=production webpack",
    "format": "prettier --write './src/**/*.js'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prod": "NODE_ENV=production webpack && NODE_ENV=production node server/main.js",
    "start": "NODE_ENV=development node server/main.js",
    "test": "jest --watch"
  },
  "jest": {
    "clearMocks": true,
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "testURL": "http://localhost"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.6.5",
    "react-testing-library": "^5.8.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.22.2"
  }
}
